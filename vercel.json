{
  "buildCommand": "npm run db:generate && npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev:full",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
